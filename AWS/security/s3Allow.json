{
   "Version": "2012-10-17",
   "Statement": [
    {
    "Sid":"s3ListBucketAndListObjectAndPutObject", 
    "Effect": "Allow",
    "Action" :[
        "s3:ListBucket",
        "s3:PutObject", 
        "s3:GetObject"],
    "Resource":[
        "arn:aws:s3:::demo2391",
        "arn:aws:s3:::demo2391/*"
    ]   
   },
   {
    "Sid":"Deny2Object", 
    "Effect":"Deny",
    "Action":["s3:GetObject"],
    "Resource":["arn:aws:s3:::demo2391/demo.tfstate"]
   }
]
}

